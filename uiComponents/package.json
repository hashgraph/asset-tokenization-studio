  "scripts": {
    "build": "rm -fr lib && tsc",
    "build-storybook": "storybook build",
    "dev": "yarn storybook",
    "dev-watch": "yarn prepare-exports && concurrently 'yarn dev-watch-rollup' 'yarn dev'",
    "dev-watch-rollup": "yarn rollup --watch --watch.onEnd 'yalc push'",
    "install-peers": "install-peers -f",
    "lint": "eslint --ext .ts --ext .tsx src",
    "prepack": "clean-package",
    "prepare-exports": "node scripts/editPackage.js",
    "postpack": "clean-package restore",
    "rollup": "rm -fr Components Theme Hooks && rollup -c rollup.config.js",
    "storybook": "storybook dev -p 6006",
    "test:local": "npx jest",
    "test": "npx jest  --detectOpenHandles",
    "test:components": "TZ=UTC npx jest --detectOpenHandles --watchAll --testPathIgnorePatterns='src/(?!Components)'",
    "theme": "install-peers -f && chakra-cli tokens src/Theme/theme.ts",
    "theme:watch": "install-peers -f && chakra-cli tokens src/Theme/theme.ts --watch",
    "peers": "yarn && yarn install-peers"
  },
